{% if terms %}

    <div class="alignfull">
        <div class="w-full max-w-screen-xl mx-auto p-8 bg-gray-50">
            <div class="swiper-container activitiesSlider max-w-screen-xl mx-auto">
                <!-- If we need navigation arrows -->
                <div class="hidden ml-auto lg:flex justify-between hopes--swiper-navigation bottom-5">
                    <div class="swiper-button-prev relative"></div>
                    <div class="swiper-button-next relative"></div>
                </div>
                <div class="swiper-wrapper">

                    {% for term in terms %}
                        {% set permalink = function('get_the_permalink', post.ID) %}
                        {% set thumbnail = function('get_the_post_thumbnail_url', post.ID) %}
                        <div class="bg-cover bg-blue bg-no-repeat swiper-slide activities-item" style="{% if term.bg %}background-color:{{ term.bg }}{% endif %}; background-image:url({{ term.cv.url }})">
                            <div class="text-center mt-8 mb-2">
                                <div class="h-14 w-14 bg-yellow rounded-full flex items-center justify-center mx-auto">
                                    <svg class="mx-auto" width="40" height="40" viewBox="0 0 512 512" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                                        <title>study</title>
                                        <g id="maquettes-HOPES-ALUMNI" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                                            <g id="Home-nonconnecte-b" transform="translate(-349.000000, -3787.000000)">
                                                <image id="study" x="349" y="3787" width="512" height="512" xlink:href="data:image/png;base64,"></image>
                                            </g>
                                        </g>
                                    </svg>
                                </div>

                            </div>
                            <h4 class="text-white text-center text-xl leading-hfour mb-0 w-8/12 mx-auto">{{ term.name }}</h4>
                            <div class="text-center pt-5">
                                <a href="{{ site.link }}/skills/?skill={{ term.slug }}"
                                   class="w-7/12 text-right relative btn uppercase bg-blue-light btn-mini text-lg text-white asap font-normal">
                                    <span class="btn-mask absolute top-0 left-0 w-0 h-full z-10 bg-blue"></span>
                                    <span class="relative z-30">{{ __("Discover", "region") }}</span>
                                </a>
                            </div>


                        </div>
                    {% endfor %}

                </div>
            </div>
        </div>
    </div>

{% endif %}